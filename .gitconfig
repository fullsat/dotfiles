[alias]
  vlog = log --pretty=format:\"%C(red)%h%Creset %C(cyan)<%an>%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset\" --abbrev-commit --date=relative
  vg = !git vlog
  glog = !git vlog --graph
  ga = !git glog --all
  
  st = status -s -b
  stat = status
  ci = commit
  co = checkout
  br = branch
  ls = ls-files
  fe = fetch -p
